{
	"background":{
		"scripts": ["chromeController.js"]
	},
	"page_action": {
		"default_icon": "img/Chrome/enabled.png",
		"default_title": "__MSG_tooltip__"
	},
	"content_scripts": [ {
		"all_frames": true,
		"js": [ "content.js" ],
		"matches": [ "http://*/*", "https://*/*" ],
		"run_at": "document_start",
		"css": ["stylesheets/theelementsoftypographicstylesheet.css"]
	}, {
		"all_frames": true,
		"matches": [ "http://*.wikipedia.org/*", "https://*.wikipedia.org/*" ],
		"run_at": "document_start",
		"css": ["stylesheets/wikipedia.css"]
	} ],
	"default_locale": "en",
	"description": "__MSG_extDescription__",
	"icons": {
		"512": "img/icon512.png",
		"128": "img/icon128.png",
		"48": "img/icon48.png"
	},
	"name": "__MSG_extName__",
	"permissions": [ "tabs", "http://*/", "https://*/" ],
	"version": "1.3",
	"manifest_version": 2
}
